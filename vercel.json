{
  "version": 2,
  "name": "credit-card-assistant",
  "builds": [
    {
      "src": "cardgpt-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cardgpt-ui/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://cardgpt-india-production.up.railway.app"
  }
}